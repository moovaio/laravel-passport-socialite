{
  "name": "moova/laravel-passport-socialite",
  "description": "This is a Laravel passport grant for social.",
  "keywords": [ "passport", "laravel", "social" ],
  "type": "library",
  "license": "MIT",
  "require": {
    "laravel/passport": "^12.0",
    "laravel/socialite": "^5.20",
    "illuminate/support": "^12.0",
    "league/oauth2-server": "^8.0 || ^8.3 || ^9.2"
  },
  "autoload": {
    "psr-4": {
      "Larva\\Passport\\Socialite\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Larva\\Passport\\Socialite\\PassportSocialiteServiceProvider"
      ]
    }
  },
  "prefer-stable": true
}
